<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/home_frg"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/near_black"
    android:padding="20dp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <androidx.fragment.app.FragmentContainerView
            android:id="@+id/fragmentContainerView"
            android:name="mobile_project.music_app_project.Fragment.Fragment_div_user_welcome"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <androidx.fragment.app.FragmentContainerView
            android:id="@+id/fragmentContainerView2"
            android:name="mobile_project.music_app_project.Fragment.Fragment_div_suggestions"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <androidx.fragment.app.FragmentContainerView
            android:id="@+id/fragmentContainerView3"
            android:name="mobile_project.music_app_project.Fragment.Fragment_div_category"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <androidx.fragment.app.FragmentContainerView
            android:id="@+id/fragmentContainerView4"
            android:name="mobile_project.music_app_project.Fragment.Fragment_div_artist"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <androidx.fragment.app.FragmentContainerView
            android:id="@+id/fragmentContainerView5"
            android:name="mobile_project.music_app_project.Fragment.Fragment_div_playlist"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

<!--        <androidx.fragment.app.FragmentContainerView-->
<!--            android:id="@+id/fragmentContainerView6"-->
<!--            android:name="mobile_project.music_app_project.Fragment.Fragment_div_rating"-->
